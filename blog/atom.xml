---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Blog about Prototype01 by opencarecc</title>
	<link href="{{ site.github.url }}/blog/atom.xml" rel="self"/>
	<link href="{{ site.github.url }}/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.github.url }}/blog</id>
	<author>
		<name>opencare.cc</name>
		<email>info@opencare.cc</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="{{ site.baseurl }}{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>{{ site.baseurl }}{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
